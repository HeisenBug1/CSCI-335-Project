<div class="info-wrapper xs-my2" data-tracking-props="{&quot;prop59&quot;:&quot;small_list::1::list item::35::Content-Venue-24231::tile::true::::::::false&quot;,&quot;eVar59&quot;:&quot;D=c59&quot;,&quot;pev2&quot;:&quot;list item&quot;,&quot;prop60&quot;:&quot;American Museum of Natural History&quot;,&quot;eVar60&quot;:&quot;D=c60&quot;,&quot;prop61&quot;:&quot;Content-Venue-24231&quot;,&quot;eVar61&quot;:&quot;D=c61&quot;}">
    <div class="xs-text-7 xs-line-height-6 xs-text-charcoal description-wrapper xs-relative xs-flex xs-flex-column">
