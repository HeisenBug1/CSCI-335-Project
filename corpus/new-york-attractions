                                    };_TOAds.loadSlot(window.googletag, window.pbjs, slotConfig);

        });
    </script>
<!-- marked ad slot id ad-mpu-0 -->

    

                                                                                            



    





                    <div
        id="ad-mpu-1"
        class="ad module ad-mpu ad--desktop mpu-1" data-component="{&quot;componentInfo&quot;:{&quot;componentID&quot;:&quot;ad-mpu-1&quot;,&quot;componentName&quot;:&quot;side&quot;},&quot;category&quot;:{&quot;componentType&quot;:&quot;advert&quot;}}"
    >
    </div>

        <script>
        _TOAds.cmd.push(function() {
            const adParams = {"id":"ad-mpu-1","group":"side2","type":"side","lazyLoad":300,"breakpoints":["md","lg"]};

            if (_TOAds.lazyLoadOffsets && _TOAds.lazyLoadOffsets[adParams.group]) {
                adParams.lazyLoad = _TOAds.lazyLoadOffsets[adParams.group];
            }

            const slotConfig = _TOAds.generateSlotConfig(adParams);slotConfig.shouldAdvertRender = function() {
                                        return (2 === 1 && !_TOAds.isPrevented('side'));
                                    };slotConfig.shouldPreloadAdvert = {"adId":"ad-mpu-2","distance":7000};slotConfig.onAfterRender = function(e) {
        window.dispatchEvent(new CustomEvent('advert-rendered', {
            detail: { type: 'side' }
        }));
    };slotConfig.onAfterPreload = function(preloader, currentSlot) {
                                        if (!preloader.elem.classList.contains('xs-hide')) {
                                            preloader.elem.classList.add('xs-hide');
                                        }
                                        if (currentSlot.elem.classList.contains('xs-hide')) {
                                            currentSlot.elem.classList.remove('xs-hide');
                                        }
                                    };_TOAds.loadSlot(window.googletag, window.pbjs, slotConfig);

        });
    </script>
