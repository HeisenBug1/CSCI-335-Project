    <h3 class="card-title xs-text-charcoal title--compressed xs-text-2 xs-line-height-2 xs-mb2">
                    <a href="/newyork/outdoor/south-street-seaport" target="_blank" rel="#{noFollow} noopener&quot;" data-tracking="{&quot;eventInfo&quot;:{&quot;eventName&quot;:&quot;list item&quot;,&quot;eventAction&quot;:&quot;contentClick&quot;,&quot;timestamp&quot;:1603436989,&quot;type&quot;:&quot;custom&quot;},&quot;attributes&quot;:{&quot;contentType&quot;:&quot;venue&quot;,&quot;entityId&quot;:&quot;35721&quot;,&quot;contentId&quot;:&quot;Content-Venue-35721&quot;,&quot;contentUrl&quot;:&quot;https:\/\/www.timeout.com\/newyork\/outdoor\/south-street-seaport&quot;,&quot;contentName&quot;:&quot;South Street Seaport&quot;,&quot;contentPrimaryCategory&quot;:&quot;things to do (category)&quot;,&quot;subCategory1&quot;:null,&quot;contentIsSponsored&quot;:false,&quot;entityZone&quot;:&quot;small_list&quot;,&quot;entityZoneName&quot;:&quot;small_list&quot;,&quot;entityZonePosition&quot;:1,&quot;entityElement&quot;:&quot;list item&quot;,&quot;entityPosition&quot;:42,&quot;entityInteractionTarget&quot;:&quot;tile&quot;,&quot;entityIsBookable&quot;:true,&quot;expandable&quot;:&quot;&quot;,&quot;aboveTheFold&quot;:&quot;&quot;,&quot;linesOfText&quot;:&quot;&quot;,&quot;entityIsSponsored&quot;:false}}"     data-track-click='{&quot;prop59&quot;:&quot;small_list::1::list item::42::Content-Venue-35721::tile::true::::::::false&quot;,&quot;eVar59&quot;:&quot;D=c59&quot;,&quot;pev2&quot;:&quot;list item&quot;,&quot;prop60&quot;:&quot;South Street Seaport&quot;,&quot;eVar60&quot;:&quot;D=c60&quot;,&quot;prop61&quot;:&quot;Content-Venue-35721&quot;,&quot;eVar61&quot;:&quot;D=c61&quot;}'
