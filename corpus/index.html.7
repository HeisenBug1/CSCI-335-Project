            <script>
                // what's the purpose of two almost identical blocks
                // set button color
                $(".featured-post").each( function () {
                    if ( $(this).hasClass( 'active' ) ) {
                        buttons = $(this).find( "button" );
                        buttons.each( function () {
                            if ( $(this).data( 'filter' ) == 1 ) {
                                $(this).find("div div").css( "background-color", "#ff8320" );
                            }
                        })
                    }
                });

                // changing featured button by click
                $(".featured-posts-navigation button").on("click", function () {

                    clearInterval(timerId);

                    let currentPostId = $(this).data("filter");
                    let currentPostIdSelector = '#featured-post-' + currentPostId;

                    // hide all slides
                    $(".featured-post").each(function () {
                        if ($(this).hasClass('active')) {
                            $(this).removeClass('active');
                            $(this).css('display', 'none');
                        }
                    });

                    // show the next slide
                    $(currentPostIdSelector).fadeIn("5000", function () {
                       $(currentPostIdSelector).addClass('active');

                        // change button color
                        $(currentPostIdSelector).each(function () {
                            if ($(this).hasClass('active')) {
                                buttons = $(this).find("button");
                                buttons.each(function () {
                                    if ($(this).data('filter') == currentPostId) {
                                        $(this).find("div div").css("background-color", "#ff8320");
                                    }
                                })
                            }
                        });

                        timerId = setInterval(slideChange, 50000);
                    });

                });

                function slideChange() {

                    let currentPostId = 1;
                    let slidesCount = $(".featured-post").length;

                    // get current slide id
                    $(".featured-post").each(function () {
                        if ( $(this).hasClass('active') ) {
                            currentPostId = $(this).data('item');
                            if ( currentPostId != slidesCount ) {
                                currentPostId++;
                            } else {
                                currentPostId = 1;
                            }
                        }
                    });

                    let currentPostIdSelector = '#featured-post-' + currentPostId;

                    // hide all slides
                    $(".featured-post").each(function () {
                        if ($(this).hasClass('active')) {
                            $(this).removeClass('active');
                            $(this).css('display', 'none');
                        }
                    });

                    // show the next slide
                    $(currentPostIdSelector).fadeIn("5000", function () {
                       $(currentPostIdSelector).addClass('active');

                        // change button color
                        $(currentPostIdSelector).each(function () {
                            if ($(this).hasClass('active')) {
                                buttons = $(this).find("button");
                                buttons.each(function () {
                                    if ($(this).data('filter') == currentPostId) {
                                        $(this).find("div div").css("background-color", "#ff8320");
                                    }
                                })
                            }
                        });
                    });
                }

                let timerId = setInterval(slideChange, 50000);

            </script>
            <script>
                //sticky header
                $(window).scroll(function() {
                    var top = $(document).scrollTop();
                    if (top < 100) {
                        if ( $(".header-search-overlay").hasClass( 'sticky' ) ) $(".header-search-overlay").removeClass( 'sticky' );

                        //remove sticky class on menu
                        $(".main-menu").css({top: '0', position: 'relative', background: 'none' });
                        if ( $(".main-menu").hasClass( 'sticky' ) ) $(".main-menu").removeClass( 'sticky' );
                    }
                    else {
                        //add sticky class to search overlay to display that on sticky menu
                        if ( ! $(".header-search-overlay").hasClass( 'sticky' ) ) $(".header-search-overlay").addClass( 'sticky' );

                        //add sticky class to menu
                        $(".main-menu").css({top: '0', position: 'fixed', background: '#fff' });
                        if ( ! $(".main-menu").hasClass( 'sticky' ) ) $(".main-menu").addClass( 'sticky' );
                    }
                });
