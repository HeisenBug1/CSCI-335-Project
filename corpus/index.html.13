 $(document).ready(function() {

  function createCookie(name, value, days) {
   var expires;
   if (days) {
    var date = new Date();
    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
    expires = '; expires=' + date.toGMTString();
   }
   else {
    expires = '';
   }
   document.cookie = encodeURIComponent(name) + "=" + encodeURIComponent(value) + expires + '; path=/';
  }

  function readCookie(name) {
   var keyValue = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
   return keyValue ? keyValue[2] : null;
  }

  // Inactivity monitor
  var idleTime = 0;
  // var timeout=8; // 8 seconds
  var timeout=25; // 25 seconds
  function doPoll() {
   idleTime = idleTime + 1;
   if (idleTime >= timeout) {
//    $(window).off('scroll');
    $('#promo_itineraries_slidin').hide();
   }
   else {
    setTimeout(doPoll, 1000); // 1 second
   }
  }
  $('#promo_itineraries_slidin').mousemove(function () {
   idleTime = 0;
  });
  $('#promo_itineraries_slidin').keypress(function () {
   idleTime = 0;
  });
  // /Inactivity monitor
  if (readCookie('promo_itineraries_shown') != '1' && parseInt(readCookie('cta_count_r')) > 1) {
    $('#promo_itineraries_slidin').delay(8000).animate({'bottom':'0'}, 1000, function(){
/*
     $(window).scroll(function() {
      $(window).off('scroll');
      $('#promo_itineraries_slidin').hide();
     });
*/
     doPoll();
    });
    // $('#promo_itineraries_slidin').delay(5000).animate({'bottom':'0'}, 1000);
  }

  // close button functionality
  $("#promo_itineraries_slidin_close").click(function(){
//      $(window).off('scroll');
      //  $('#promo_itineraries_slidin').animate({'bottom':'-500px'}, 1000);
      $('#promo_itineraries_slidin').hide();
      createCookie('promo_itineraries_shown', '1', false);
  });

  // form processing (new slide-in)

  $('#promo_itineraries_slidin_button').click(function(){

      $.post('/promo-itineraries.php', {'email':$('#promo_itineraries_slidin_email').val(),'gdpr':$("input[name='gdpr_check']:checked").val()},function(data){
        if (data.error != '') {
            $('#promo_itineraries_slidin_error').html('<strong>'+data.error+'</strong>').show();
        } else {
            $('#promo_itineraries_slidin_form').hide(function(){
                $('#promo_itineraries_slidin_close').hide();
                $('#promo_itineraries_slidin_thanks').show().delay(4000).fadeOut('slow',function(){
                    $('#promo_itineraries_slidin').hide();
                    createCookie('promo_itineraries_shown', '1', 365);
                });
                /**
                $('#promo_itineraries_slidin_thanks').fadeIn();
                createCookie('promo_itineraries_shown', '1', 365);
                **/

            });
        }

      });

  });

 });
</script>


