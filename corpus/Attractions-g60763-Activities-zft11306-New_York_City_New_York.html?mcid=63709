define('ta/util/ASDF', ['vanillajs', 'utils/asdf-encoder', 'ta/util/Cookie', 'ns-exporter', 'ta/servlet/Popup'],
function(vanilla, Encoder, cookie, nsExporter, taPopup) {
var exports= {
asdf: function(str, pid) {
if (pid) {
cookie.setPIDCookie(pid);
}
window.location = Encoder.asdf(str);
},
asdfPopup: function(str, pid, name, width, height) {
if (pid) {
cookie.setPIDCookie(pid);
}
taPopup.open(Encoder.asdf(str), name, width, height);
},
asdfPopupTerms: function(str) {
taPopup.open(Encoder.asdf(str),'terms',300,300,30,25);
},
asdfDcd: function(str) {
return Encoder.asdf(str);
}
};
nsExporter.exportTo(exports, "ta.util.ASDF");
return exports;
});
