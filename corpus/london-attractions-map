    <header class="cf">
        <h1 class="article-h1">London attractions map</h1>
        
    </header>

        <div class="cf intro-block">
            
    <div class="map feature-map" data-latlng="51.515419,-0.141099" data-zoom="12" data-nobounds='true' ></div>
    <div id="map-top10s" class="cf">
            <label><input type="checkbox" value="points-london-top-attractions"/> Top attractions</label>
            <label><input type="checkbox" value="points-londons-historic-houses"/> Historic houses</label>
            <label><input type="checkbox" value="points-royal-london"/> Royal London</label>
            <label><input type="checkbox" value="points-londons-best-afternoon-tea"/> Best afternoon tea</label>
            <label><input type="checkbox" value="points-top-department-stores"/> Top department stores</label>
