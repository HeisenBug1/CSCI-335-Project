            width  = mapSlot[0][1][0];
            height = mapSlot[0][1][1];
        } else if (mapSlot) {
            width  = mapSlot[1][1][0];
            height = mapSlot[1][1][1];
        }
    
    
    

    
    if (mapSlot) {
        googletag.defineSlot('/61567524/nycgo.com/PhotoGallery', [width, height], 'gpt-unit-top-sm').defineSizeMapping(mapSlot)
    

