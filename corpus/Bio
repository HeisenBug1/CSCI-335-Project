        $("#Welcome_Holder").on('click keydown', function () {


        })

        WE_Email.onblur = function () {
            if (this.value == '') {
                if (currentUserEmail.indexOf("@") >= 0) {
                    WE_Email.value = currentUserEmail;
                } else {
                    WE_Email.value = "";
                }
            }

        }
