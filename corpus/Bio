                <div id="found" class="scroll-pause content-section">

                    <div id="found-background-group">
                        <div id="found-davinci"></div>
                        <div id="found-billsign" class="scroll-front"></div>
                        <div id="found-bill-melinda" class="scroll-middle"></div>
