<script type="text/javascript">
	
    $(window).resize(function () {
    });
    $(document).ready(function () {
        var ww = $(window).width();
        var wh = $(window).height();
		$("#Auth_50_WECTA_Left_linkLI").click(function () {
                hideAuthHolders();
resendVerifyLink();
        });
    });
</script>
