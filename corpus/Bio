        function doOmnitureFollowEvent(event) {
            try {
                s.linkTrackVars = "events,eVar9";
                s.linkTrackEvents = "event11,event12,event13,event15";
                s.events = event;
                s.tl(true, 'e', 'followexitlink:' + event);
            } catch (e) {
                console.log(e.message);
                console.log(e.name);
            }
    }


