function ALS_success(fLoc) {
    var whatF= 'Signedupvia' + fLoc;
    var s = s_gi(s_account);
    s.linkTrackVars = 'eVar4,prop21,events';
    s.linkTrackEvents = 'event19';
    s.eVar4 = 'AL SIGNUP';
    s.prop21 = whatF;
    s.events = 'event19';
    s.tl(true, 'o', whatF);
    console.log("success from: " + fLoc);
}

