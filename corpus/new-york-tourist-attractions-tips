oseconds":0},"character_count":17912,"cover_images":{"default":{"__typename":"MediaArtemisImage","alt_tag":"Brooklyn Bridge","cover_image_type":"default","image_id":"2680204"},"pinterest":{"__typename":"MediaArtemisImage","alt_tag":null,"cover_image_type":"pinterest","image_id":"2669308"}},"cover_videos":null,"created":{"seconds":1497643839,"nanoseconds":0},"description":"A New Yorker’s opinion is the best way to judge if tourist traps like the Empire State Building and Statue of Liberty are worth your time.","duration":null,"edition":"Nation","elements":[{"__typename":"ParagraphMainImage","alt_tag":"Brooklyn Bridge","caption":"Brooklyn Bridge","clickthrough_url":null,"created":1499899558,"credit_title":"IM_photo/Shutterstock","credit_url":"https://www.shutterstock.com/image-photo/brooklyn-bridge-new-york-city-aerial-235760404?src=k3h-NeSDshFh5m-WHwPNsg-1-6","filename":"2668779.jpg","focal":{"x":2000,"y":1420},"gif_overlay":false,"has_mobile_image":true,"has_video":false,"height":2849,"image_credit":null,"image_id":"2679758","mobile_image":{"__typename":"ParagraphMainImage","alt_tag":"Brooklyn Bridge","caption":"Brooklyn Bridge","clickthrough_url":null,"created":1499899558,"credit_title":"IM_photo/Shutterstock","credit_url":null,"filename":"2668779.jpg","focal":{"x":2000,"y":1420},"gif_overlay":false,"has_video":false,"height":2849,"image_credit":null,"image_id":"2679758","style":"standard","title":null,"updated":1499899577,"uuid":"4574273e-be34-4121-8d16-da750cd779c8-mobile","variable_height":false,"video":null,"width":4000},"style":"full_width","title":null,"updated":1499899577,"uuid":"4574273e-be34-4121-8d16-da750cd779c8","variable_height":false,"video":null,"width":4000},{"__typename":"ParagraphEditorsNote","format":"full_html","text":"\u003cem\u003eIf you're even thinking of being in NYC, check out the rest of our \u003ca href=\"https://www.thrillist.com/travel/nation/destination-new-york-city-visit-nyc-travel-guide\" target=\"_blank\"\u003eDestiNATION: New Yor